  "scoring_components": {
    "PHQ2_Total": {
      "description": "PHQ-2 Depression Screening Score",
      "calculation": "Sum of PHQ2_Q1 + PHQ2_Q2",
      "range": [0, 6],
      "clinical_cutoff": 3,
      "interpretation": {
        "0-2": "Negative screen (low likelihood of depression)",
        "3-6": "Positive screen (further evaluation recommended)"
      }
    },
    "High_Risk_Flags": {
      "description": "Count of major risk factors present",
      "flags": [
        {
          "flag_name": "FLAG_PRIOR_MH",
          "condition": "BPG_MH == 2",
          "description": "Prior mental health treatment"
        },
        {
          "flag_name": "FLAG_MH_MEDS",
          "condition": "PRE_RX == 2 AND PRE_RX_MH in ['Yes', 'Not sure']",
          "description": "Medications for mood/anxiety"
        },
        {
          "flag_name": "FLAG_POOR_HEALTH",
          "condition": "HTH_GEN >= 4",
          "description": "Fair or Poor general health"
        }
      ],
      "range": [0, 3]
    },
    "Lifestyle_Risk": {
      "description": "Count of lifestyle risk factors",
      "factors": [
        {
          "factor_name": "No_Exercise",
          "condition": "PRE_EXER == 1",
          "points": 1
        },
        {
          "factor_name": "Dieting",
          "condition": "PRE_DIET == 2",
          "points": 1
        },
        {
          "factor_name": "Overweight_Obese",
          "condition": "OWGT_OBS == 2",
          "points": 1
        }
      ],
      "range": [0, 3]
    }
  },
  "classification_rules": {
    "HIGH_RISK": {
      "description": "User meets criteria for high-risk classification",
      "rules": [
        {
          "rule_id": "RULE_1",
          "name": "Positive PHQ-2 Screen",
          "condition": "PHQ2_Total >= 3",
          "priority": 1
        },
        {
          "rule_id": "RULE_2",
          "name": "Any High-Risk Flag",
          "condition": "High_Risk_Flags >= 1",
          "priority": 2
        },
        {
          "rule_id": "RULE_3",
          "name": "Borderline + Multiple Lifestyle Risks",
          "condition": "PHQ2_Total == 2 AND Lifestyle_Risk >= 2",
          "priority": 3
        }
      ],
      "logic": "ANY rule true â†’ HIGH_RISK"
    },
    "LOW_RISK": {
      "description": "User does not meet high-risk criteria",
      "condition": "NOT HIGH_RISK"
    }
  },
